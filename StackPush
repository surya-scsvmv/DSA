#include <stdio.h>
#define MAX 5

int s[MAX], top = -1;

void push(int item) {
    if (top == MAX - 1) {
        printf("stack overflow\n");
        return;
    }
    top++;
    s[top] = item;
    printf("pushed %d\n", item);
}

int main() {
    int item, n;
    printf("how many items to push? ");
    scanf("%d", &n);

    if (n > MAX) {
        printf("Cannot push more than %d items\n", MAX);
        n = MAX;
    }

    for (int i = 0; i < n; i++) {
        printf("enter item: ");
        scanf("%d", &item);
        push(item);
    }
    return 0;
}
